{
  "name": "destiny-power-bars",
  "homepage": "https://destiny-power-bars.corke.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predeploy": "npm run vite:build",
    "deploy": "gh-pages -d dist",
    "prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "eslint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook",
    "vite:dev": "vite --https --port 3000 --strictPort --open",
    "vite:build": "vite build",
    "vite:preview": "vite preview"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,css,json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {}
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@rooks/use-interval": "^4.11.2",
    "bungie-api-ts": "^4.21.0",
    "classnames": "^2.2.6",
    "dom-to-image": "^2.6.0",
    "file-saver": "^2.0.2",
    "humanize-duration": "^3.24.0",
    "idb-keyval": "^3.2.0",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "random-seed": "^0.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "simple-query-string": "^1.3.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.0.28",
    "@types/classnames": "^2.2.11",
    "@types/dom-to-image": "^2.6.2",
    "@types/file-saver": "^2.0.1",
    "@types/humanize-duration": "^3.18.1",
    "@types/jest": "26.0.15",
    "@types/lodash": "^4.14.164",
    "@types/node": "^14.14.6",
    "@types/preval.macro": "^3.0.0",
    "@types/random-seed": "^0.3.3",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/simple-query-string": "^1.3.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.0",
    "cross-env": "^7.0.2",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.3",
    "eslint": "8.29",
    "eslint-config-prettier": "^6.15.0",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "prettier": "2.8",
    "process": "^0.11.10",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "tslib": "^2.2.0",
    "typescript": "4.9",
    "vite": "^4.3.4",
    "vite-plugin-info": "^0.4.0",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.2.0",
    "webpack": "^5.75.0"
  }
}
