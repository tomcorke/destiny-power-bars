{
  "name": "destiny-power-bars",
  "homepage": "https://destiny-power-bars.corke.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env HTTPS=true react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "tslint": "tslint \"src/**/*.{ts,tsx}\"",
    "tslint:fix": "tslint --fix \"src/**/*.{ts,tsx}\"",
    "storybook": "start-storybook"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,css,json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@use-it/interval": "^1.0.0",
    "bungie-api-ts": "^4.9.0",
    "classnames": "^2.2.6",
    "dom-to-image": "^2.6.0",
    "file-saver": "^2.0.2",
    "humanize-duration": "^3.24.0",
    "idb-keyval": "^3.2.0",
    "lodash": "^4.17.20",
    "node-sass": "^7.0.0",
    "normalize.css": "^8.0.1",
    "random-seed": "^0.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.2.0",
    "react-scripts": "^4.0.0",
    "simple-query-string": "^1.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@types/classnames": "^2.2.11",
    "@types/dom-to-image": "^2.6.2",
    "@types/file-saver": "^2.0.1",
    "@types/humanize-duration": "^3.18.1",
    "@types/jest": "26.0.15",
    "@types/lodash": "^4.14.164",
    "@types/node": "^14.14.6",
    "@types/node-sass": "^4.11.1",
    "@types/preval.macro": "^3.0.0",
    "@types/random-seed": "^0.3.3",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/simple-query-string": "^1.3.0",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "prettier": "^2.1.2",
    "preval.macro": "^5.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.0.5"
  }
}
