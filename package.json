{
  "name": "destiny-power-bars",
  "homepage": "https://destiny-power-bars.corke.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env HTTPS=true react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "eslint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "storybook": "cross-env NODE_OPTIONS=--openssl-legacy-provider start-storybook"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,css,json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@use-it/interval": "^1.0.0",
    "bungie-api-ts": "^4.21.0",
    "classnames": "^2.2.6",
    "dom-to-image": "^2.6.0",
    "file-saver": "^2.0.2",
    "humanize-duration": "^3.24.0",
    "idb-keyval": "^3.2.0",
    "lodash": "^4.17.20",
    "normalize.css": "^8.0.1",
    "random-seed": "^0.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^5.0.1",
    "simple-query-string": "^1.3.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.28",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.0.28",
    "@types/classnames": "^2.2.11",
    "@types/dom-to-image": "^2.6.2",
    "@types/file-saver": "^2.0.1",
    "@types/humanize-duration": "^3.18.1",
    "@types/jest": "26.0.15",
    "@types/lodash": "^4.14.164",
    "@types/node": "^14.14.6",
    "@types/preval.macro": "^3.0.0",
    "@types/random-seed": "^0.3.3",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/simple-query-string": "^1.3.0",
    "cross-env": "^7.0.2",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.7.3",
    "eslint": "8.29",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^7.0.1",
    "gh-pages": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.0",
    "prettier": "2.8",
    "preval.macro": "^5.0.0",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "tslib": "^2.2.0",
    "typescript": "4.9",
    "webpack": "^5.75.0"
  }
}